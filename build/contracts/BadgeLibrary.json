  "source": "pragma solidity ^0.4.15;\n\nlibrary BadgeLibrary {\n\n    /// @notice represents simple details about a earnable badge\n    struct Badge {\n        address issuer;\n        string description;\n        string name;\n        string image;\n        string version; \n        // bytes32 linkHash;\n        // bytes32 chainHash;\n    }    \n\n    /// @notice represents details of an issued badge \n    struct Credential {\n        Badge badge;\n        uint expires;\n        address recipient;\n        bytes32 txnKey;\n    }\n    \n    /// @notice compute hash of badgename \n    function getBadgeNameHash(string _badgename) constant public returns (bytes32 _hash) {\n        return keccak256(_badgename);\n    }\n\n    /// @notice compute hash for badge linking\n    function getBadgeLinkHash(string _name, address _issuer) constant public returns (bytes32 hash) {\n        return keccak256(_name, _issuer);\n    }\n\n    /// @notice compute integrity hash of credential data\n    function getIntegrityHash(\n        address issuer, \n        string description, \n        string name, \n        string image, \n        string version,\n        address recipient\n    ) constant public returns(bytes32 _hash) \n    {\n        return keccak256(\n                issuer, \n                description, \n                name, \n                image, \n                version, \n                recipient\n        );\n    }\n}",
